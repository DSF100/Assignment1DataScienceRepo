    "list_data_2016 = []\n",
    "list_data_2021 = []\n",
    "\n",
    "with open(LANGUAGE_DATA_2016_C, newline='') as csvfile:\n",
    "    csvreader = csv.reader(csvfile, delimiter=',')\n",
    "    data = list(csvreader)\n",
    "    for name in data[0]:\n",
    "        if name[0] == \"P\":\n",
    "            list_data_2016.append(name)\n",
    "\n",
    "with open(LANGUAGE_DATA_2016_D, newline='') as csvfile:\n",
    "    csvreader = csv.reader(csvfile, delimiter=',')\n",
    "    data = list(csvreader)\n",
    "    for name in data[0]:\n",
    "        if name[0] == \"P\":\n",
    "            list_data_2016.append(name)\n",
    "\n",
    "with open(LANGUAGE_DATA_2021_C, newline='') as csvfile:\n",
    "    csvreader = csv.reader(csvfile, delimiter=',')\n",
    "    data = list(csvreader)\n",
    "    for name in data[0]:\n",
    "        if name[0] == \"P\":\n",
    "            list_data_2021.append(name)\n",
    "\n",
    "with open(LANGUAGE_DATA_2021_D, newline='') as csvfile:\n",
    "    csvreader = csv.reader(csvfile, delimiter=',')\n",
    "    data = list(csvreader)\n",
    "    for name in data[0]:\n",
    "        if name[0] == \"P\":\n",
    "            list_data_2021.append(name)\n",
    "\n",
    "with open(LANGUAGE_DATA_2021_E, newline='') as csvfile:\n",
    "    csvreader = csv.reader(csvfile, delimiter=',')\n",
    "    data = list(csvreader)\n",
    "    for name in data[0]:\n",
    "        if name[0] == \"P\":\n",
    "            list_data_2021.append(name)\n",
    "\n",
    "print(len(list_data_2016))\n",
    "print(len(list_data_2021))\n",
    "#print(list_data_2016)\n",
    "#print(list_data_2021)\n",
    "\n",
    "for i in range(len(list_data_2021)):\n",
    "    list_data_2021[i] = list_data_2021[i].replace(\"UOLSE\", \"SOLSE\")\n",
    "\n",
    "list_data_2021_original_copy = list_data_2021\n",
    "\n",
    "for name in list_data_2021_original_copy:\n",
